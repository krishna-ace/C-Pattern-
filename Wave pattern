#include <stdio.h>
int main() {
    int n = 12;
    int i, j;

    for (i = 1; i <= 4; i++) {
        for (j = 1; j <= n; j++) {
            if ((i == 1 && j % 6 == 4) ||
                (i == 2 && j % 6 == 3) ||
                (i == 3 && j % 6 == 2) ||
                (i == 4 && j % 6 == 1))
                printf("*");
            else
                printf(" ");
        }
        printf("\n");
    }

    return 0;
}
