#include <stdio.h>
#include <math.h>

int main() {
    int r = 5;

    for(int i = -r; i <= r; i++) {
        for(int j = -r; j <= r; j++) {
            if(i*i + j*j <= r*r)
                printf("*");
            else
                printf(" ");
        }
        printf("\n");
    }

    return 0;
}
