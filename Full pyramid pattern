#include <stdio.h>
int main() {
    int n = 5;
    int i = 1;
    while (i <= n) {
        int space = 1;
        while (space <= n - i) {
            printf(" ");
            space++;
        }
        int star = 1;
        while (star <= (2 * i - 1)) {
            printf("*");
            star++;
        }
        printf("\n");
        i++;
    }
    return 0;
}
