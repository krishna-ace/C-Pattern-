#include <stdio.h>
int main() {
    int i, j;
    for (i = 1; i <= 3; i++) {
        for (j = 1; j <= 17; j++) {
            if ((j >= 6 - i && j <= 6 + i) ||
                (j >= 12 - i && j <= 12 + i))
                printf("*");
            else
                printf(" ");
        }
        printf("\n");
    }
    for (i = 7; i >= 1; i--) {
        for (j = 1; j <= 13; j++) {
            if (j >= 8 - i && j <= 6 + i)
                printf("*");
            else
                printf(" ");
        }
        printf("\n");
    }
    return 0;
}
