#include <stdio.h>

int main() {
    int n = 4;
    int i, j;
    int size = 2 * n + 1;

    for (i = 0; i < size; i++) {
        for (j = 0; j < size; j++) {
            if (i == 0 || i == size - 1 || j == 0 || j == size - 1)
                printf("*");
            else if (j >= n - i && j <= n + i && i <= n)
                printf("*");
            else if (j >= i - n && j <= 3 * n - i && i > n)
                printf("*");
            else
                printf(" ");
        }
        printf("\n");
    }

    return 0;
}
