#include <stdio.h>
int main() {
    int n = 5, i, j;
    int w = 2 * n + 1;

    for (i = 0; i <= n; i++) {
        for (j = 0; j < w; j++) {
            if (i == 0 || i == n)
                printf("*");
            else if (j == 0 || j == w - 1)
                printf("*");
            else if (j >= n - i + 1 && j <= n + i - 1)
                printf("*");
            else
                printf(" ");
        }
        printf("\n");
    }

    return 0;
}
